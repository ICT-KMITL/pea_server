{
  "name": "pea_server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --content-base src --inline --hot",
    "build": "NODE_ENV=production webpack --progress -p --config ./webpack.config.js",
    "build-windows": "set NODE_ENV=production&&webpack --progress -p --config ./webpack.config.js",
    "watch": "NODE_ENV=production webpack --watch --progress -p --config ./webpack.config.js",
    "watch-windows": "set NODE_ENV=production&&webpack --watch --progress -p --config ./webpack.config.js",
    "dev": "webpack --progress -d --config ./webpack.config.js",
    "dev-server": "webpack-dev-server -d --content-base public_html --inline --port 80 --host 0.0.0.0 --history-api-fallback",
    "dev-watch": "webpack --watch --progress -d --config ./webpack.config.js"
  },
  "dependencies": {
    "axios": "0.5.x",
    "bootstrap": "^3.3.7",
    "highcharts": "^5.0.2",
    "jquery": "^3.1.0",
    "mobx": "^2.6.5",
    "mobx-react": "^4.0.3",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "react-tabs": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.6",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "~6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.18.0",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "react-addons-test-utils": "^15.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-node-externals": "^1.5.4"
  }
}
